n = read_line.to_i64

ans = 0_i64
[
  {1_000_i64, 999_999_i64, 1_i64},
  {1_000_000_i64, 999_999_999_i64, 2_i64},
  {1_000_000_000_i64, 999_999_999_999_i64, 3_i64},
  {1_000_000_000_000_i64, 999_999_999_999_999_i64, 4_i64},
  {1_000_000_000_000_000_i64, 999_999_999_999_999_999_i64, 5_i64},
  {1_000_000_000_000_000_000_i64, Int64::MAX, 6_i64},
].each do |(min, max, commas)|
  ans += [0_i64, ([max, n].min - min + 1) * commas].max
end
p ans